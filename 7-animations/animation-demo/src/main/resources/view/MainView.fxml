<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="233.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep12.fx.animation.controller.MainViewController">
   <children>
      <Label fx:id="lblTitle" alignment="CENTER" layoutX="160.0" layoutY="39.0" text="Java FX Animations" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="40.0">
         <font>
            <Font name="Ubuntu Bold" size="30.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" layoutX="203.0" layoutY="107.0" spacing="10.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
         <children>
            <Button mnemonicParsing="false" onAction="#btnPlayOnAction" text="PLAY">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#btnStopOnAction" text="STOP">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Label fx:id="lblTime" alignment="CENTER" layoutX="282.0" layoutY="177.0" text="Label" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font name="Ubuntu" size="23.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
